<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
	<servlet>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>com.github.wyxpku.searchapi.api.SearchApi</servlet-class>
	</servlet>	
 
	<servlet>
		<servlet-name>InitServelt</servlet-name>
		<servlet-class>com.github.wyxpku.searchapi.api.InitServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/rangesearch</url-pattern>
	</servlet-mapping>
	
</web-app>
